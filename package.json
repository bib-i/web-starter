{
  "name": "web-starter",
  "version": "0.3.0",
  "author": "Dylan Richardson",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "env": "node scripts/env.js",
    "deploy:db": "prisma deploy",
    "build:web": "next build",
    "build": "npm-run-all env --parallel build:web deploy:db",
    "start:web": "node server/web.js",
    "start:api": "node server/api.js",
    "start": "npm-run-all build --parallel start:web start:api",
    "dev:web": "nodemon server/web.js",
    "dev:api": "nodemon server/api.js",
    "dev": "npm-run-all build --parallel dev:web dev:api",
    "test": "NODE_ENV=test ava"
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.8",
    "apollo-link-context": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.3.1",
    "dotenv": "^5.0.0",
    "graphql-yoga": "^1.4.3",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^5.0.0",
    "prisma-binding": "^1.5.12",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "write-yaml": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "clear-require": "^2.0.0",
    "glob": "^7.1.2",
    "graphql-cli": "^2.15.7",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "prisma": "^1.2.8"
  },
  "ava": {
    "require": ["babel-register"],
    "babel": "inherit"
  },
  "resolutions": {
    "graphql": "0.13.1"
  }
}
